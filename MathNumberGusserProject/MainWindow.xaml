<Window x:Class="MathNumberGusserProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        
        mc:Ignorable="d"
        WindowStyle="None"
        WindowState="Normal" 
        WindowStartupLocation="CenterScreen"
        Title="QuizGenerate" Height="300" Width="300">
    <Grid>
        <StackPanel VerticalAlignment="Center">
            <TextBlock Text="Enter the Number of Questions Generate : " FontSize="12" FontWeight="Bold"></TextBlock>
            <TextBox x:Name="quiznumber"></TextBox>
            <Button  Click="GenerateClick">Generate</Button>
        </StackPanel>
    </Grid>
  
</Window>
